.dashboard
  .row
    .col-md-5
      %h2
        Start Automatic Like !
      .okcupid-box-auto{:role => "form"}
        .form-group
          %label{:for => "inputUsername"} OkCupid Username / Email
          %input#inputUsername.form-control{:placeholder => "Enter email or username", :type => "text"}/
        .form-group
          %label{:for => "inputPassword"} Password
          %input#inputPassword.form-control{:placeholder => "Password", :type => "password"}/
        %button.btn.btn-default{:type => "submit", "ng-click" => ""} Start Automatic Likes
    .col-md-5.col-md-offset-2
      %h2
        Reports
      %ul.list-unstyled
        %li
          You have no reports
